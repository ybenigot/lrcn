name: "lrcn_resnet_to_lstm"

# test data layers
layer {
  name: "data1"
  type: "ImageData"
  top: "data"
  top: "label"
  transform_param {
    mirror: true
    crop_size: 224
    mean_value: 104
    mean_value: 117
    mean_value: 123
  }
  image_data_param {
    source: "/media/yves/sandisk3/caffe-master/examples/coco_caption/h5_data/buffer_16/test_aligned_20_batches/image_list.with_dummy_labels.txt"
    batch_size: 16
    new_height: 256
    new_width: 256
  }
}
layer {
  name: "data2"
  type: "HDF5Data"
  top: "cont_sentence"
  top: "input_sentence"
  top: "target_sentence"
  hdf5_data_param {
    source: "/media/yves/sandisk3/caffe-master/examples/coco_caption/h5_data/buffer_16/test_aligned_20_batches/hdf5_chunk_list.txt"
    batch_size: 16
  }
}

layer {
  name: "silence"
  type: "Silence"
  bottom: "label"
}


